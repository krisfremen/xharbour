
!include $(INET_DIR)\env\makeb32.env

lib: $(INETLIB) 

$(INETLIB): $(INETOBJ)
   @if exist $(INETLIB) erase $(INETLIB)
   $(lib) "$(INETLIB)" /P512 @&&!
+$(**: = &^
+)

!

clean:
   @if exist $(OBJDIR)\*.obj erase $(OBJDIR)\*.obj
   @if exist $(OBJDIR)\*.c   erase $(OBJDIR)\*.c
   @if exist $(OBJDIR)\*.res erase $(OBJDIR)\*.res
   @if exist $(OBJDIR)\*.map erase $(OBJDIR)\*.map
   @if exist $(OBJDIR)\*.rws erase $(OBJDIR)\*.rws
   @if exist $(INETLIB) erase $(INETLIB)
