 $(SRC06)\_WinRect.c $(SRC06)\_WinPrn.c $(SRC06)\_WinPen.c $(SRC06)\_WinMsg.c $(SRC06)\_WinMous.c $(SRC06)\_WinMisc.c $(SRC06)\_WinMeta.c $(SRC06)\_WinMenu.c $(SRC06)\_WinMem.c $(SRC06)\_WinMAPI.c //
 $(SRC06)\_WinMain.c $(SRC06)\_WinLV.c $(SRC06)\_WinKbrd.c $(SRC06)\_WinIni.c $(SRC06)\_WinILst.c $(SRC06)\_WinIcon.c $(SRC06)\_WinHead.c $(SRC06)\_WinGDI.c $(SRC06)\_WinFont.c $(SRC06)\_WinDraw.c //
