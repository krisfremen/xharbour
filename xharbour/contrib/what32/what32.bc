 $(OBJ)\_WinRect.obj $(OBJ)\_WinPrn.obj $(OBJ)\_WinPen.obj $(OBJ)\_WinMsg.obj $(OBJ)\_WinMous.obj $(OBJ)\_WinMisc.obj $(OBJ)\_WinMeta.obj $(OBJ)\_WinMenu.obj $(OBJ)\_WinMem.obj $(OBJ)\_WinMAPI.obj //
