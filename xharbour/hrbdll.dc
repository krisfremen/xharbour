# makefile for creating Harbour.dll with DigitalMars C/C++ compiler
#

ECHO = echo
DEL = del

#
# NOTE: This USR_FLAGS is used for testing compatibility of DMC DLL
# with other xHarbour program compiled with NON-DMC compiler
#
USR_FLAGS    = -D"HB_FUNC_NO_DECORATION"

HARBOUR_EXE  = $(BIN_DIR)\harbour.exe
CC           = $(DMCMAIN)\BIN\SC.EXE
DMLINK       = $(DMCMAIN)\BIN\LINK.EXE
DMLIB        = $(DMCMAIN)\BIN\LIB.EXE
DMLIB_DIR    = $(DMCMAIN)\LIB
INCLUDE_DIR  = $(XHARBOUR_ROOT)\INCLUDE
HARBOURFLAGS = -i$(INCLUDE_DIR) -n1 -q0 -w -es2 -gc0
MAINFLAGS    = -i$(INCLUDE_DIR) -n  -q0 -w -es2 -gc0
CFLAGS       = -D"__EXPORT__" -Jm -cpp -mn -I"$(INCLUDE_DIR)" -c
CFLAGS       = -6 -WD -o $(CFLAGS) -D"__WIN32__" -D"WIN32" $(USR_FLAGS)

HARBOUR_CMD = $(HARBOUR_EXE) $(HARBOURFLAGS) $** -o$@
CC_CMD      = $(CC) $(CFLAGS) -o$@ $**

HARBOUR_DLL = $(BIN_DIR)\harbour.dll
HRBDLL_LIB  = $(LIB_DIR)\harbour.lib
HRBDLL_DEF  = $(LIB_DIR)\harbour.def

HBRUNDLL_EXE    = $(BIN_DIR)\hbrundll.exe
HBTESTDLL_EXE   = $(BIN_DIR)\hbtestdll.exe
HBMAKEDLL_EXE   = $(BIN_DIR)\hbmakedll.exe
HBDOCDLL_EXE    = $(BIN_DIR)\hbdocdll.exe
XBSCRIPTDLL_EXE = $(BIN_DIR)\xbscriptdll.exe

DMC_LIBRARY = \
   snn.lib + \
   kernel32.lib + \
   winspool.lib + \
   user32.lib + \
   advapi32.lib + \
   ole32.lib + \
   gdi32.lib + \
   uuid.lib + \
   wsock32.lib + \
   oleaut32.lib

