#
# makefile for creating Harbour.dll with PellesC compiler
#

!ifndef ECHO
# don't indent definitions
ECHO = echo.
!endif

!ifndef DEL
# You might need to set 'ECHO=echo.' - but don't set 'DEL=del.' !!!
DEL = del
!endif

HARBOUR_EXE  = $(HB_DIR)\BIN\harbour.exe
POCC_EXE     = $(POCC_DIR)\BIN\POCC.EXE
POLINK_EXE   = $(POCC_DIR)\BIN\POLINK.EXE
POLIB_EXE    = $(POCC_DIR)\BIN\POLIB.EXE
POLIB_DIR    = $(POCC_DIR)\LIB

HARBOURFLAGS = -i$(HB_DIR)\include -n1 -q0 -w -es2 -gc0 -D"__EXPORT__"
MAINFLAGS    = -i$(HB_DIR)\include -n -q0 -w -es2 -gc0
CFLAGS       = /Ze /Go /Tx86-coff /I"$(HB_DIR)\include" /I"$(POCC_DIR)\INCLUDE" /I"$(POCC_DIR)\INCLUDE\WIN" /D"__EXPORT__" /D"__WIN32__" /D"WIN32" /D"__WINDOWS__"
DLLFLAGS     = /DLL /LIBPATH:$(POLIB_DIR) /LIBPATH:$(POLIB_DIR)\WIN /MACHINE:IX86 /SUBSYSTEM:WINDOWS
EXEFLAGS     = /LIBPATH:$(POLIB_DIR) /LIBPATH:$(POLIB_DIR)\WIN /MACHINE:IX86 /SUBSYSTEM:CONSOLE

HARBOUR_CMD = $(HARBOUR_EXE) $(HARBOURFLAGS) $** -o$@
POCC_CMD    = $(POCC_EXE) $(CFLAGS) /Fo$@ $**

HARBOUR_DLL  = $(LIB_DIR)\harbour.dll
HRBDLL_LIB   = $(LIB_DIR)\harbour.lib

HBRUNDLL_EXE    = $(BIN_DIR)\hbrundll.exe
HBTESTDLL_EXE   = $(BIN_DIR)\hbtestdll.exe
HBMAKEDLL_EXE   = $(BIN_DIR)\hbmakedll.exe
HBDOCDLL_EXE    = $(BIN_DIR)\hbdocdll.exe
XBSCRIPTDLL_EXE = $(BIN_DIR)\xbscriptdll.exe


POCC_LIBRARY = \
   kernel32.lib \
   winspool.lib \
   user32.lib \
   advapi32.lib \
   ole32.lib \
   gdi32.lib \
   uuid.lib \
   ws2_32.lib \
   mpr.lib \
   oleaut32.lib

