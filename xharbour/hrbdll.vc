HBTEST_DIR        = utils\hbtest

# ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
# ³ obj directory ³Û
# ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙÛ
#  ßßßßßßßßßßßßßßßßß
# Please adjust accordingly
# It is highly suggested that the object directory be different than that
# of static library since __EXPORT__ is defined here which we might not
# neccesarily did on static libraries

OBJ_DIR           = obj\dll\vc
EXE_OBJ_DIR       = obj\dll\vc\bin

# ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
# ³ include directory ³Û
# ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙÛ
#  ßßßßßßßßßßßßßßßßßßßßß

# This should be standard

INCLUDE_DIR  = include

# ÚÄÄÄÄÄÄÄÄ¿
# ³ output ³Û
# ÀÄÄÄÄÄÄÄÄÙÛ
#  ßßßßßßßßßß
# Please adjust accordingly
LIB_DIR         = lib\vc
HARBOUR_LIB     = $(LIB_DIR)\harbour.lib
HARBOUR_DLL     = $(BIN_DIR)\dll\harbour.dll
HBRUNDLL_EXE    = $(BIN_DIR)\hbrundll.exe
HBTESTDLL_EXE   = $(BIN_DIR)\hbtestdll.exe
HBMAKEDLL_EXE   = $(BIN_DIR)\hbmakedll.exe
HBDOCDLL_EXE    = $(BIN_DIR)\hbdocdll.exe
XBSCRIPTDLL_EXE = $(BIN_DIR)\xbscriptdll.exe

# ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
# ³ compiler flags ³Û
# ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙÛ
#  ßßßßßßßßßßßßßßßßßß
# Make sure to compile PRG with /n1 switch otherwise the application will
# not work. HOWEVER, when creating application, we should use /n or /n0
# otherwise xHarbour will not create start-up, so application fails to
# execute.
# Make sure to define __EXPORT__ or we cannot access xHarbour functions
# from DLL

HARBOURFLAGS  = /D__EXPORT__ /i$(INCLUDE_DIR) /q0 /w /es2 /n1
CLIBFLAGS     = /D__EXPORT__ /DHB_FM_STATISTICS_OFF /MT /FD /G6 /Ogt2yb1p /Gs /TP /W3 /nologo /c /I$(INCLUDE_DIR) /D__WINDOWS__ /DWIN32 /D__WIN32__ $(HB_MULTI_GT) $(CFLAGS)
MAINFLAGS     = -i$(INCLUDE_DIR) -n  -q0 -w -es2 -gc0
HARBOUR_CMD   = $(HARBOUR_EXE) $(HARBOURFLAGS) $** -o$@
HARBOUR_BATCH_CMD = $(HARBOUR_EXE) $(HARBOURFLAGS) -o$(OBJ_DIR)\ $<
CC_CMD        = $(CC) $(CLIBFLAGS) /Fo$@ $**
CLIBFLAGSDLL  = /DHB_FM_STATISTICS_OFF /MT /FD /G6 /Ogt2yb1p /Gs /TP /W3 /nologo /c /I$(INCLUDE_DIR) /D__WINDOWS__ /DWIN32 /D__WIN32__
CC_CMD_DLL    = $(CC) $(CLIBFLAGSDLL) /DHB_EXEMEM_USED /Fo$@ $**
CC_BATCH_CMD  = $(CC) $(CLIBFLAGS) /Fo$(OBJ_DIR)\ $<

# ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
# ³  HARBOUR_DLL rules  ³Û
# ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙÛ
#  ßßßßßßßßßßßßßßßßßßßßßßß

