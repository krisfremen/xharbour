C_USR        = -dHB_WIN32_IO -dHB_FM_STATISTICS_OFF -d__WIN32__ -d__EXPORT__ -dHB_NO_PROFILER -dHB_NO_TRACE
CFLAGS       = -bd -j -w3 -d1 -5 -fp5 -s -onaehtzr -oi+ -ei -zp8 -zq -zt0 -mf -bt=NT
CLIBFLAGS    = $(CFLAGS) $(C_USR) -i$(INCLUDE_DIR)
