OBJ_DIR           = obj\dll\watcom

# ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
# ³ include directory ³Û
# ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙÛ
#  ßßßßßßßßßßßßßßßßßßßßß

INCLUDE_DIR = include

# ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
# ³ compiler flags ³Û
# ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙÛ
#  ßßßßßßßßßßßßßßßßßß

C_USR        = -dHB_FM_STATISTICS_OFF -d__WIN32__ -d__EXPORT__ -dHB_NO_PROFILER -dHB_NO_TRACE
CFLAGS       = -bd -j -w3 -d1 -5 -fp5 -s -onaehtzr -oi+ -ei -zp8 -zq -zt0 -mf -bt=NT
CLIBFLAGS    = $(CFLAGS) $(C_USR) -i$(INCLUDE_DIR)
HARBOURFLAGS = /D__EXPORT__ /DHB_FM_STATISTICS_OFF /i$(INCLUDE_DIR) /q0 /w /es2 /n1 /gc0
MAINFLAGS    = -i$(INCLUDE_DIR) -n  -q0 -w -es2 -gc0

HARBOUR_CMD = $(HARBOUR_EXE) $(HARBOURFLAGS) $** -o$@
CC_CMD      = $(CC) $(CLIBFLAGS) -fo$@ $**

# ÚÄÄÄÄÄÄÄÄ¿
# ³ output ³Û
# ÀÄÄÄÄÄÄÄÄÙÛ
#  ßßßßßßßßßß
# Please adjust according to your environment

