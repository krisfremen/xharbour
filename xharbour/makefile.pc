!ifndef ECHO
# don't indent definitions
ECHO = echo.
!endif

!ifndef DEL
# You might need to set 'ECHO=echo.' - but don't set 'DEL=del.' !!!
DEL = del
!endif

POCC_EXE     = $(POCC_DIR)\BIN\POCC.EXE
POLINK_EXE   = $(POCC_DIR)\BIN\POLINK.EXE
POLIB_EXE    = $(POCC_DIR)\BIN\POLIB.EXE
POLIB_DIR    = $(POCC_DIR)\LIB


HARBOURFLAGS = -i$(HB_DIR)\INCLUDE -n1 -q0 -w -es2 -gc0
MAINFLAGS    = -i$(HB_DIR)\INCLUDE -n -q0 -w -es2 -gc0
CFLAGS       = /Ze /Zx /Go /Tx86-coff /I"$(HB_DIR)\INCLUDE" /I"$(POCC_DIR)\INCLUDE" /I"$(POCC_DIR)\INCLUDE\WIN" /D"__WIN32__"
LINKFLAGS    = /LIBPATH:$(POLIB_DIR) /LIBPATH:$(POLIB_DIR)\WIN /MACHINE:IX86 /SUBSYSTEM:CONSOLE

!ifdef HB_MT
CFLAGS = $(CFLAGS) /D"HB_THREAD_SUPPORT" /MT
!endif

