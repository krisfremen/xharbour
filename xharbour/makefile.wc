#
# C compiler definition and C flags. These should never have to change.
#

CFLAGS               = -j -w3 -d1 -5 -fp5 -s -onaehtzr -oi+ -ei -zp8 -zq -zt0 -mf -bt=NT -D__WIN32__
CLIBFLAGS            = $(CFLAGS) -i$(INCLUDE_DIR)
HARBOURFLAGS         = -i$(INCLUDE_DIR) -n -q0 -w -es2 -gc0 $(PRG_USR) $(HARBOURFLAGS)

HVM_OBJ_FLAGS        = $(CLIBFLAGS) -dSIMPLEX
HVM_MT_OBJ_FLAGS     = $(CLIBFLAGS) -dSIMPLEX -dHB_THREAD_SUPPORT
HVM_GUI_OBJ_FLAGS    = $(CLIBFLAGS) -dSIMPLEX -dHB_GUI
HVM_GUI_MT_OBJ_FLAGS = $(CLIBFLAGS) -dSIMPLEX -dHB_THREAD_SUPPORT -dHB_GUI

!ifdef HB_THREAD_SUPPORT
CLIBFLAGS = -bm -dHB_THREAD_SUPPORT $(CLIBFLAGS)
!endif

!if $(HB_LEX) == SIMPLEX
CLIBFLAGS = $(CLIBFLAGS) -dSIMPLEX
!endif

!ifdef HB_DOC_PDF
HARBOURFLAGS = $(HARBOURFLAGS) -dPDF
!endif

!ifdef HB_THREAD_SUPPORT
HB_MT_FLAGS=-dHB_THREAD_SUPPORT
HARBOURFLAGS= $(HARBOURFLAGS) $(HB_MT_FLAGS)
!endif

