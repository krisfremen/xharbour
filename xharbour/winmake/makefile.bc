!if $d(DLL_MT)
CLIBFLAGS   =$(CLIBFLAGS) -tWM -DHB_THREAD_SUPPORT
!else
CLIBFLAGS   =$(CLIBFLAGS) -tWD
!endif
!ifdef HB_DIR_ADS
ACEH_DIR    =-I"$(HB_DIR_ADS)"
!endif
!ifdef HB_DIR_OPENSSL
OPENSSL_DIR    =-I"$(HB_DIR_OPENSSL)\include"
!endif
!if !("$(HB_DOC_PDF)"=="")
HBDOC_PDFLIB_LIB=$(PDFLIB_LIB)
HBDOC_EXE_OBJS=\
	$(HBDOC_EXE_OBJS)\
	$(OBJ_DIR)\genpdf1.obj\
	$(OBJ_DIR)\pdfhbdoc.obj
!endif

INCLUDE_DIR =-I"include" -I"$(OBJ_DIR)" -I"$(ZLIB_DIR)"
DLLFLAGS    =-C -aa -Tpd -Gn -Gi -L$(LIB_DIR) -L$(BIN_DIR)
EXEFLAGS    =-ap -Tpe -Gn -C -L$(LIB_DIR) -L$(BIN_DIR)
CLIBFLAGS   =$(CLIBFLAGS) -DHAVE_CONFIG_H -D__WIN32__ -DHB_NO_DUPLICATE_HVMPROCESSSYMBOL -D__EXPORT__ -q -a8 -OS -O2 -6 -c $(ACEH_DIR) $(OPENSSL_DIR) $(INCLUDE_DIR) -d -w3 -DOPENSSL_NO_DEPRECATED -D_HAVE_SQLITE_CONFIG_H
HARBOURFLAGS=$(HARBOURFLAGS) -iinclude -q0 -w2 -es2 -gc0

# User flags should be able to override everything!
DLLFLAGS    =$(DLLFLAGS) $(LFLAGS)
EXEFLAGS    =$(EXEFLAGS) $(LFLAGS)
CLIBFLAGS   =$(CLIBFLAGS)  $(CFLAGS)
LINKERFLAGS =$(LINKERFLAGS) $(LFLAGS)

CC_CMD      =BCC32 $(CLIBFLAGS) -o$@ $**
CCC_CMD     =$(CC_CMD)
HB_CMD      =$(HARBOUR_EXE) -n1 $(LINEFLAGS) $(HARBOURFLAGS) $** -o$@
HB_CMD_MAIN =$(HARBOUR_EXE) -n0 $(LINEFLAGS) $(HARBOURFLAGS) $** -o$@
BISON_CMD1  =bison -v -d $** -o$@
BISON_CMD2  =bison -v -p hb_comp -d $** -o$@
DLL_LINK_CMD=ILINK32 $(LINKERFLAGS) -L"$(CC_DIR)\lib" -L"$(CC_DIR)\lib\psdk" "$(CC_DIR)\lib\c0d32.obj" $**,$@,,$(COMPILERLIBS)
DLLEXE_LINK_CMD=ILINK32 $(LINKERFLAGS) -L"$(CC_DIR)\lib" -L"$(CC_DIR)\lib\psdk" "$(CC_DIR)\lib\c0x32.obj" $**,$@,,$(USE_DLL_LIB) $(HBDOC_PDFLIB_LIB) $(HARBOUR_DLL_LIB) $(COMPILERLIBS)

#===============================================================================
# Compiler and 3rd Party libraries need to be linked
#===============================================================================
!if $d(HB_CG32)
COMPILERLIBS=\
	cg32.lib\
	$(COMPILERLIBS)\
	cw32mt.lib\
	import32.lib
!else
COMPILERLIBS=\
	$(COMPILERLIBS)\
	cw32mt.lib\
	import32.lib
!endif

#===============================================================================
# The Project
#===============================================================================
ALL: $(DLL_PROJECT)

#===============================================================================
# HARBOUR.DLL rules
#===============================================================================
$(HARBOUR_DLL) : $(HARBOUR_DLL_OBJS)
	ILINK32 $(DLLFLAGS) -L"$(CC_DIR)\lib" -L"$(CC_DIR)\lib\psdk" @&&!
"$(CC_DIR)\lib\c0d32.obj" $**, $@,, $(COMPILERLIBS)
!

