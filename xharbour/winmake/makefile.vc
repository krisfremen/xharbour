!endif
!else
CLIBFLAGS=$(CLIBFLAGS) -MT
!endif
!endif

!ifdef HB_DIR_ADS
ACEH_DIR    =-I"$(HB_DIR_ADS)"
!endif
!ifdef HB_DIR_OPENSSL
OPENSSL_DIR    =-I"$(HB_DIR_OPENSSL)\include"
!endif

INCLUDE_DIR =include

DLLFLAGS=\
 	/MAP\
	/FORCE:MULTIPLE\
	/OUT:$(HARBOUR_DLL)\
	/DLL\
	/IMPLIB:$(HARBOUR_DLL_LIB)\
	/NOLOGO\
        /LIBPATH:"$(CC_DIR)\LIB" /LIBPATH:$(LIB_DIR) $(ACE_LIBPATH)\
	/SUBSYSTEM:WINDOWS\
	$(LFLAGS) $(MANIFESTFLAGS)\
	$(HARBOUR_DLL_OBJS)

EXEFLAGS=\
	/NOLOGO /LIBPATH:"$(CC_DIR)\LIB"\
	/LIBPATH:$(LIB_DIR)\
	/SUBSYSTEM:CONSOLE

CLIBFLAGS   =$(CLIBFLAGS) -c -DHAVE_CONFIG_H $(ACEH_DIR) $(OPENSSL_DIR) -Iinclude -I"$(CC_DIR)\include" -I$(OBJ_DIR) -DWIN32 -D__WIN32__ -DHB_NO_DUPLICATE_HVMPROCESSSYMBOL -D__EXPORT__ $(OPTFLAGS) -Gs -W3 -DOPENSSL_NO_DEPRECATED -nologo $(CFLAGS)
HARBOURFLAGS=$(HARBOURFLAGS) -i$(INCLUDE_DIR) -q0 -w2 -es2 -gc2

CC_CMD     =CL $(CLIBFLAGS) -Fo$@ $**
CCC_CMD    =CL $(CLIBFLAGS) -TC -Fo$@ $**
HB_CMD     =$(HARBOUR_EXE) -n1 $(LINEFLAGS) $(HARBOURFLAGS) $** -o$@
HB_CMD_MAIN=$(HARBOUR_EXE) -n0 $(LINEFLAGS) $(HARBOURFLAGS) $** -o$@
BISON_CMD1 =bison -v -d $** -o$@
BISON_CMD2 =bison -v -p hb_comp -d $** -o$@
DLL_LINK_CMD=LINK $(DLLFLAGS)
DLLEXE_LINK_CMD=LINK $(EXEFLAGS) $(LFLAGS) /OUT:$@ $** $(USE_DLL_LIB) $(HARBOUR_DLL_LIB)

#===============================================================================
# The Project
