INCLUDE_DIR =include
INCLUDE_DIR =-I$(INCLUDE_DIR) -I"$(CC_DIR)\include"
!if ("$(USE_MSVCRT)"=="1")
CLIBFLAGS=$(CLIBFLAGS) -MD
!endif
!if ("$(CPLUSPLUS)"=="1")
