#===============================================================================
# The Project
#===============================================================================
!if !("$(HB_THREAD_SUPPORT)"=="1")
PROJECT=$(ST_PROJECT)
!else
PROJECT=$(MT_PROJECT)
!endif

ALL: $(PROJECT)

#===============================================================================
# C compiler definition and C flags. These should never have to change.
#===============================================================================

#===============================================================================
# Append your own build flags here
#===============================================================================
HARBOURFLAGS=$(HARBOURFLAGS)
CLIBFLAGS   =$(CLIBFLAGS)

#===============================================================================
# Option to use MSVCRT rather than LIBC*.LIB
#===============================================================================
!if ("$(__MSC__)"=="6")	|| ("$(__MSC__)"=="7") || ("$(__MSC__)"=="71")
OPTFLAGS=-Ogt2yb1p
!else
OPTFLAGS=-Otyb1
!endif
INCLUDE_DIR =include
INCLUDE_DIR =-I$(INCLUDE_DIR) -I$(OBJ_DIR) -I"$(CC_DIR)\include"
!if ("$(USE_MSVCRT)"=="1")
CLIBFLAGS=$(CLIBFLAGS) -MD
!else
CLIBFLAGS=$(CLIBFLAGS) -MT
!endif
!if ("$(CPLUSPLUS)"=="1")
CLIBFLAGS=$(CLIBFLAGS) -TP
!endif
CLIBFLAGS   =$(CLIBFLAGS) -DHAVE_CONFIG_H -c $(OPTFLAGS) -GA -Gs -W3 -nologo $(INCLUDE_DIR)
HARBOURFLAGS=$(HARBOURFLAGS) -iinclude -q0 -w2 -es2 -gc0
!if ("$(HB_THREAD_SUPPORT)"=="1")
CLIBFLAGS   =$(CLIBFLAGS) -DHB_THREAD_SUPPORT
!endif

LINKERFLAGS=$(LINKERFLAGS)\
	/NOLOGO /LIBPATH:"$(CC_DIR)\LIB" /LIBPATH:$(LIB_DIR) /SUBSYSTEM:CONSOLE

!if ("$(USE_MSVCRT)"=="")
LINKERFLAGS=$(LINKERFLAGS) /FORCE:MULTIPLE
!endif

# User flags should be able to override everything!
CLIBFLAGS   =$(CLIBFLAGS)  $(CFLAGS)
LINKERFLAGS =$(LINKERFLAGS) $(LFLAGS)

#-------------------------------------------------------------------------------
# AJ: 2008-04-26
# NOTE ON HB_CMD/HB_CMD_MAIN:
#  -n1 is used for non-executing module (fe. library's modules), because in
#  some cases, if the module is compiled with -n, it will execute upon
#  initialization, thus RTE.
#  -n is ONLY applied to main module to make sure that only that module will
#  execute upon running the executable.
#-------------------------------------------------------------------------------
HB_CMD       =$(HARBOUR_EXE) -n1 $(LINEFLAGS) $(HARBOURFLAGS) $** -o$@
HB_CMD_MAIN  =$(HARBOUR_EXE) -n $(LINEFLAGS) $(HARBOURFLAGS) $** -o$@
!if ("$(__MSC__)"=="8") || ("$(__MSC__)"=="9") || ("$(__MSC__)"=="10")
!if ("$(USE_MSVCRT)"=="1")
MANIFESTFLAGS=/MANIFEST /MANIFESTFILE:"$(OBJ_DIR)\intermediate.manifest"
MT_CMD       =MT -nologo -manifest "$(OBJ_DIR)\intermediate.manifest" -outputresource:$@;1
!else
MANIFESTFLAGS=/MANIFEST:NO
!endif
!endif
CC_CMD       =CL $(CLIBFLAGS) -Fo$@ $**
CCC_CMD      =CL $(CLIBFLAGS) -TC -Fo$@ $**
CC_CMD_FMSTAT=CL $(CLIBFLAGS) -DHB_FM_STATISTICS -Fo$@ $**
LIB_CMD      =LIB /NOLOGO /OUT:$@ $**
LINK_CMD     =LINK /NOLOGO /OUT:$@ $** $(LINKERFLAGS) $(MANIFESTFLAGS)
BISON_CMD1   =bison -v -d $** -o$@
BISON_CMD2   =bison -v -p hb_comp -d $** -o$@

COMPILERLIBS=\
	user32.lib\
	ws2_32.lib\
	winspool.lib\
	ole32.lib\
	oleaut32.lib\
	advapi32.lib\
	winmm.lib\
	mpr.lib
HRB_LIBS=$(COMMON_LIB) $(PP_LIB) $(COMPILER_LIB)
EXE_LIBS=$(HARBOUR_LIBS)
CMN_LIBS=$(COMMON_LIB)

#===============================================================================
# Library dependencies and build rules
#===============================================================================
